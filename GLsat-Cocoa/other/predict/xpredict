#Eterm -g 80x25 -F 10x20 -n PREDICT -x -e predict $1 $2 $3 $4
#gnome-terminal --geometry 80x25 --disable-sound -t PREDICT -e predict $1 $2 $3 $4
#rxvt -geometry 80x25 -font 10x20 -rv -tn linux -title PREDICT -e predict $1 $2 $3 $4
#rxvt -geometry 80x25 -font 10x20 -rv -tn color_xterm -title PREDICT -e predict $1 $2 $3 $4
#kvt -vt_geometry 80x25 -linux -T PREDICT -e predict $1 $2 $3 $4
